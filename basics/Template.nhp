$.return = '';

function echo(...args) {
	args.forEach(function(s){
		$.return = $.return + s;
	});
};

function exit() {
	throw new Error({});
}

$.run = (async function() {
	try { 
		<?code>
		return $.return;
	} 
	catch(e)
	{
		if(e.message != {}) {
			return '<html><head></head><body><h1>NHP Internal Error</h1><br><b>'
			+ e.stack.replace(/</gi, '&lt;').replace(/\n/gs, '<br>')
			+ '</b></body></html>';
		}
	};
});

$.run();