$.return = '';

function echo(...args) {
	args.forEach(function(s){
		$.return = $.return + s;
	});
};

function exit() {
	throw new Error({});
}

(async function() {
	await (async function() {
		try { 
			<?code>
		} 
		catch(e)
		{
			if(e.message != {}) {
				$.return = '<html><head></head><body><h1>NHP Internal Error</h1><br><b>'
				+ e.stack.replace(/</gi, '&lt;').replace(/\n/gs, '<br>')
				+ '</b></body></html>';
			}
		};
	})();
	return $.return;
})();